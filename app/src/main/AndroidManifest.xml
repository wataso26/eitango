<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.word_remember_app2">

    <application
        android:name=".RealmRememberApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Word_Remember_App">
        <activity
            android:name=".ResultActivity"
            android:exported="true" />
        <activity
            android:name=".activity_result"
            android:exported="true" />
        <activity android:name=".QuestionActivity" />
        <activity android:name=".AddedwordActivity" />
        <activity android:name=".AddwordActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>